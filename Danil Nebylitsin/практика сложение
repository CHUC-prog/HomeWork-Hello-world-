using System;
class HelloWorld {
  static void Main() {
        int первое число = 125437;
        int второе число = 1232673;

        int сумма = первое число + второе число;
        int произведение = первое число * второе число;
        int результат = произведение / сумма;

        Console.WriteLine($"Сумма: {сумма}");
        Console.WriteLine($"Произведение: {произведение}");
        Console.WriteLine($"Результат: {результат}");
  }
}
