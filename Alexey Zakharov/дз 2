class Program
{
    static void Main()
    {
        int число1 = 2;
        int число2 = 5;

        int сумма = число1 + число2;
        int произведение = число1 * число2;
        int результат = произведение / сумма;

        Console.WriteLine($"Сумма: {сумма}");
        Console.WriteLine($"Произведение: {произведение}");
        Console.WriteLine($"Результат: {результат}");
    }
}
